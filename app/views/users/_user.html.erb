<tr>
    <td><%= user.name %></td>
    <td><%= user.email %></td>
    <!-- <td><%# user.password %></td> -->
    <td class="actions">
      <%= link_to 'Show', user_path(user), class: "btn btn-primary"%>
      <%= link_to 'Edit', edit_user_path(user), class: "btn btn-success"%>
      <%= link_to 'Delete', user_path(user), method: :delete, data: {confirm: 'Are you sure'}, class: "btn btn-primary"%>
    </td>
</tr>