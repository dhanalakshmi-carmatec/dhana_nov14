<tr>
    <td><%= user.name %></td>
    <td><%= user.email %></td>
    <td class="actions">
      <%= link_to 'Show', user_path(user)%>
      <%= link_to 'Edit', edit_user_path(user)%>
      <%= link_to 'Delete', user_path(user), method: :delete, data: {confirm: 'Are you sure'}%>
      <%= link_to 'Contacts', user_contacts_path(user, @contact)%> 
    </td>
</tr>